<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.rohitbarnawal.intentweb.MainActivity"
    android:background="@drawable/bi">

    <Button
        android:layout_width="217sp"
        android:layout_height="65sp"
        android:text="CALL"
        android:id="@+id/button"
        android:background="@drawable/b_back"
        android:textSize="50dp"
        android:textStyle="bold|italic"
        android:textColorHighlight="#ffffff"
        android:textColorHint="#28a4f6"
        android:onClick="show"
        android:layout_marginLeft="1sp"
        android:layout_marginRight="1sp"
        android:layout_below="@+id/edt1"
        android:layout_toStartOf="@+id/button10" />

    <EditText
        android:layout_width="300sp"
        android:layout_height="68sp"
        android:id="@+id/edt1"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/green"
        android:inputType="number|phone|text"
        android:textColor="#6bf40a"
        android:textSize="60sp"
        android:textStyle="italic"
        android:gravity="right"
        android:hint="Enter No." />

    <Button
        android:layout_width="70sp"
        android:layout_height="70sp"
        android:text="1"
        android:id="@+id/button1"
        android:background="@drawable/green"
        android:textStyle="bold|italic"
        android:textSize="50sp"
        android:layout_below="@+id/button"
        android:layout_alignParentStart="true"
        android:layout_margin="1sp" />

    <Button
        android:layout_width="70sp"
        android:layout_height="70sp"
        android:text="2"
        android:id="@+id/button2"
        android:background="@drawable/green"
        android:textStyle="bold|italic"
        android:textSize="50sp"
        android:layout_below="@+id/button"
        android:layout_toEndOf="@+id/button1"
        android:layout_margin="1sp" />

    <Button
        android:layout_width="70sp"
        android:layout_height="70sp"
        android:text="3"
        android:id="@+id/button3"
        android:background="@drawable/green"
        android:textStyle="bold|italic"
        android:textSize="50sp"
        android:layout_marginLeft="1sp"
        android:layout_marginRight="1sp"
        android:layout_above="@+id/button5"
        android:layout_toEndOf="@+id/button0" />

    <Button
        android:layout_width="70sp"
        android:layout_height="70sp"
        android:text="4"
        android:id="@+id/button4"
        android:background="@drawable/green"
        android:textStyle="bold|italic"
        android:textSize="50sp"
        android:layout_below="@+id/button1"
        android:layout_alignParentStart="true"
        android:layout_margin="1sp" />

    <Button
        android:layout_width="70sp"
        android:layout_height="70sp"
        android:text="5"
        android:id="@+id/button5"
        android:background="@drawable/green"
        android:textStyle="bold|italic"
        android:textSize="50sp"
        android:layout_alignBottom="@+id/button4"
        android:layout_alignStart="@+id/button2"
        android:layout_margin="1sp" />

    <Button
        android:layout_width="70sp"
        android:layout_height="70sp"
        android:text="6"
        android:id="@+id/button6"
        android:background="@drawable/green"
        android:textStyle="bold|italic"
        android:textSize="50sp"
        android:layout_below="@+id/button3"
        android:layout_alignStart="@+id/button3"
        android:layout_margin="1sp" />

    <Button
        android:layout_width="70sp"
        android:layout_height="70sp"
        android:text="7"
        android:id="@+id/button7"
        android:background="@drawable/green"
        android:textStyle="bold|italic"
        android:textSize="50sp"
        android:layout_below="@+id/button4"
        android:layout_alignParentStart="true"
        android:layout_margin="1sp" />

    <Button
        android:layout_width="70sp"
        android:layout_height="70sp"
        android:text="8"
        android:id="@+id/button8"
        android:background="@drawable/green"
        android:textStyle="bold|italic"
        android:textSize="50sp"
        android:layout_alignBottom="@+id/button7"
        android:layout_alignStart="@+id/button5"
        android:layout_margin="1sp" />

    <Button
        android:layout_width="70sp"
        android:layout_height="70sp"
        android:text="9"
        android:id="@+id/button9"
        android:background="@drawable/green"
        android:textStyle="bold|italic"
        android:textSize="50sp"
        android:layout_marginLeft="1sp"
        android:layout_marginRight="1sp"
        android:layout_above="@+id/button0"
        android:layout_alignStart="@+id/button6" />

    <Button
        android:layout_width="70sp"
        android:layout_height="70sp"
        android:text="*"
        android:id="@+id/button11"
        android:background="@drawable/green"
        android:textStyle="bold|italic"
        android:textSize="50sp"
        android:layout_below="@+id/button7"
        android:layout_alignParentStart="true"
        android:layout_margin="1sp" />

    <Button
        android:layout_width="70sp"
        android:layout_height="70sp"
        android:text="0"
        android:id="@+id/button0"
        android:background="@drawable/green"
        android:textStyle="bold|italic"
        android:textSize="50sp"
        android:layout_below="@+id/button7"
        android:layout_alignStart="@+id/button8"
        android:layout_margin="1sp" />

    <Button
        android:layout_width="70sp"
        android:layout_height="70sp"
        android:text="#"
        android:id="@+id/button13"
        android:background="@drawable/green"
        android:textStyle="bold|italic"
        android:textSize="50sp"
        android:layout_marginLeft="1sp"
        android:layout_marginRight="1sp"
        android:layout_below="@+id/button9"
        android:layout_toStartOf="@+id/button10" />

    <Button
        android:layout_width="90sp"
        android:layout_height="210sp"
        android:text="C"
        android:id="@+id/button10"
        android:background="@drawable/b_back"
        android:textStyle="bold"
        android:textSize="35sp"
        android:layout_marginLeft="1sp"
        android:layout_marginRight="1sp"
        android:layout_alignBottom="@+id/button13"
        android:layout_toEndOf="@+id/button6"
        android:layout_below="@+id/button8" />

</RelativeLayout>
